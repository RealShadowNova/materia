{
	"name": "materia",
	"version": "1.0.0",
	"description": "A simple Discord bot using the Sapphire framework under discord.js",
	"author": "RealShadowNova (https://github.com/RealShadowNova/materia",
	"license": "MIT",
	"private": true,
	"main": "dist/index.js",
	"imports": {},
	"scripts": {
		"clean": "rimraf dist node_modules/.cache",
		"lint": "eslint src tests --ext ts --fix",
		"format": "eslint --write {src,tests}/**/*.ts",
		"test": "jest --verbose",
		"update": "yarn upgrade-interactive --latest",
		"build": "tsc -b src",
		"watch": "tsc -b src -w",
		"prepare": "husky install"
	},
	"dependencies": {},
	"devDependencies": {
		"@commitlint/cli": "^16.1.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@sapphire/eslint-config": "^4.0.11",
		"@sapphire/ts-config": "^3.1.8",
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.12",
		"eslint": "~8.6.0",
		"husky": "^7.0.4",
		"jest": "^27.4.7",
		"jest-circus": "^27.4.6",
		"lint-staged": "^12.3.1",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.3",
		"ts-jest": "^27.1.3",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/RealShadowNova/materia"
	},
	"engines": {
		"node": ">=16.6",
		"npm": ">=6"
	},
	"keywords": [],
	"bugs": {
		"url": "https://github.com/RealShadowNova/materia"
	},
	"homepage": "https://github.com/RealShadowNova/materia",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.ts": "eslint --fix --ext ts"
	}
}
